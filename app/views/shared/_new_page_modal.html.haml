#new-page-modal.modal{:style => "display: none;"}
  .modal-header
    %a.close{:"data-dismiss" => "modal"} &times;
    %h3= t("shared.index.modal.new_page_title")
  .modal-body
    = render "pages/form", :resource => Page.new(:context_id => current_project.try(:context).try(:id))
  .modal-footer
    = buttons Page.new
